{
  "name": "strapi-sdk-js",
  "version": "2.3.3",
  "description": "ðŸ“¦ Javascript SDK for your Strapi API",
  "keywords": [
    "strapi",
    "strapi-sdk",
    "strapi-sdk-js",
    "strapi-sdk-javascript",
    "sdk",
    "api",
    "http",
    "rest",
    "restful",
    "javascript"
  ],
  "homepage": "https://strapi-sdk-js.netlify.app",
  "bugs": {
    "url": "https://github.com/Stun3R/strapi-sdk-js/issues"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:Stun3R/strapi-sdk-js.git"
  },
  "license": "MIT",
  "author": {
    "name": "Thibaut DAVID",
    "email": "thibautdavid@icloud.com",
    "url": "https://thibautdavid.com"
  },
  "maintainers": [
    {
      "name": "Thibaut DAVID",
      "email": "thibautdavid@icloud.com",
      "url": "https://thibautdavid.com"
    }
  ],
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "start": "rollup -c --watch",
    "build": "rm -rf dist && rollup -c",
    "clean": "rm -rf bin dist coverage",
    "lint": "eslint . --ext .ts,.js",
    "lint:fix": "eslint . --ext .ts,.js --fix",
    "test": "run-s build lint test:unit",
    "test:unit": "jest",
    "commit": "git-cz",
    "docs:release": "node ./scripts/beforeRelease.js",
    "release": "dotenv release-it --",
    "prepare": "rollup -c"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint --cache --fix"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@types/js-cookie": "^3.0.6",
    "@types/qs": "^6.9.15",
    "axios": "^1.7.2",
    "defu": "^6.1.4",
    "js-cookie": "^3.0.5",
    "qs": "^6.12.1",
    "ufo": "^1.5.3"
  },
  "devDependencies": {
    "@commitlint/cli": "19.3.0",
    "@commitlint/config-conventional": "19.2.2",
    "@release-it/conventional-changelog": "8.0.1",
    "@rollup/plugin-commonjs": "^26.0.1",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@types/jest": "29.5.12",
    "@types/sinon": "17.0.3",
    "@typescript-eslint/eslint-plugin": "7.13.0",
    "@typescript-eslint/parser": "7.13.0",
    "commitizen": "4.3.0",
    "cz-conventional-changelog": "3.3.0",
    "dotenv-cli": "7.4.2",
    "esbuild": "^0.21.5",
    "eslint": "^8.56.0",
    "husky": "9.0.11",
    "jest": "29.7.0",
    "lint-staged": "15.2.6",
    "npm-run-all": "4.1.5",
    "release-it": "17.3.0",
    "rollup": "^4.18.0",
    "rollup-plugin-dts": "^6.1.1",
    "rollup-plugin-esbuild": "^6.1.1",
    "sinon": "18.0.0",
    "ts-jest": "29.1.4",
    "typescript": "5.4.5"
  }
}